{
  "name": "fake-express-http",
  "version": "1.3.1",
  "description": "Fake Express HttpRequest and HttpResponse for unit testing.",
  "main": "main/fake-express-http.js",
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch --onlyChanged",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/concept-not-found/fake-express-http"
  },
  "keywords": [
    "fake",
    "express",
    "http"
  ],
  "author": "Ronald Chen",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/concept-not-found/fake-express-http/issues"
  },
  "homepage": "https://github.com/concept-not-found/fake-express-http",
  "devDependencies": {
    "body-parser": "1.14.1",
    "eslint": "3.17.1",
    "express": "4.13.3",
    "jasmine-reporters": "2.2.0",
    "jest": "19.0.2",
    "supertest": "1.1.0",
    "supertest-as-promised": "2.0.2"
  },
  "dependencies": {
    "mime": "1.4.1",
    "verror": "1.9.0"
  },
  "jest": {
    "resetMocks": true,
    "resetModules": true,
    "collectCoverageFrom": [
      "**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "coverage/",
      "setup-jest.js",
      "main/test-controller.js"
    ],
    "coverageReporters": [
      "text-summary",
      "lcov"
    ],
    "setupTestFrameworkScriptFile": "./setup-jest.js"
  }
}
